int main() {
    int A, B, sign, n, x, sum;

    scanf("%d", & x);

    A = 1;
    B = 1;
    sum = 0;
    getchar();
    sign = 1;
    int zarib = 0;
    if ((n = getchar()) != '-') {
        A = n;
        zarib = (A - '0');
        if (A == 'x') zarib = 1;
        while((A = getchar()) != ' '){
            if (A == 'x'){
                zarib = 1;
                break;
            }
            zarib = zarib * 10 + (A - '0');
        }